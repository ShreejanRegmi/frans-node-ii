<h2>Add Category</h2>
<%= message? message: null %>
<form action="/savecategory<%= category ? `/${category.id}?_method=PATCH`:null  %>" method="POST">
    <label>Name:</label>
    <input type="text" name="name" value="<%=category?category.name:null%>" required />
    <input type="submit" value="Save Category" />
</form>